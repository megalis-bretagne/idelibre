{% extends 'form.html.twig' %}
{% import 'macro/_forms.html.twig' as macro %}

{% block pageTitle 'Modifier une structure' %}

{% block pageActions %}
    <a href="{{ path('structure_index') }}" class="btn btn-outline-primary">
        <span class="fa fa-arrow-left"></span> Retour
    </a>
{% endblock %}

{% block pageForm %}
    {{ form_start(form) }}

    {{ form_widget(form) }}

    <div class="float-right">

        {{ macro.save('structure_index') }}
    </div>

    {{ form_end(form) }}
{% endblock %}
